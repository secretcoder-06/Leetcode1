class Solution {
public:
    int lengthOfLastWord(string s) {
        int l, r;
        for (r=s.size()-1; r>=0; r--)
            if (s[r]!=' ') break;
        l=r;
        for (;l>=0;l--)
            if (s[l]==' ') break;
        return r-l;
    }
};

auto init = []()
{ 
    ios::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    return 'c';
}();
        
